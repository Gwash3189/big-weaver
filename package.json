{
  "version": "2.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "bootstrap": "rm -rf ./node_modules; npm install; npm run build",
    "setup": "npm run bootstrap",
    "server": "npm run start",
    "update": "npm run bootstrap",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "npx tsdx test --coverage",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 160,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "nextjs-backend-helpers",
  "author": "Adam Beck",
  "module": "dist/nextjs-backend-helpers.esm.js",
  "size-limit": [
    {
      "path": "dist/nextjs-backend-helpers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/nextjs-backend-helpers.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@prisma/client": "3.7.0",
    "@types/cookie": "^0.4.1",
    "cookie": "^0.4.2",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "next": "12.0.x",
    "tsyringe": "^4.6.x"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.5",
    "next": "12.0.x",
    "np": "7.6.0",
    "size-limit": "7.0.5",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  }
}
